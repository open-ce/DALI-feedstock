From: Deepali Chourasia <deepch23@in.ibm.com>
Date: Thu, 22 Jul 2021 07:14:44 -0400
Subject: [PATCH] skip test_operator_decoders_image

---
 qa/TL0_python-self-test-operators/test_body.sh        | 2 +-
 qa/TL0_python-self-test-readers-decoders/test_body.sh | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/qa/TL0_python-self-test-operators/test_body.sh b/qa/TL0_python-self-test-operators/test_body.sh
index cf97e673..504dbeb5 100644
--- a/qa/TL0_python-self-test-operators/test_body.sh
+++ b/qa/TL0_python-self-test-operators/test_body.sh
@@ -2,7 +2,7 @@
 
 test_nose() {
     # readers and decoders are tested in the TL0_python-self-test-core
-    FILTER_PATTERN="test_operator_readers_.*\.py\|test_operator_decoders_.*\.py"
+    FILTER_PATTERN="test_operator_readers_.*\.py\|test_operator_decoders_audio.py"
 
     for test_script in $(ls test_operator_*.py | sed "/$FILTER_PATTERN/d"); do
         nosetests --verbose --attr '!slow' ${test_script}
diff --git a/qa/TL0_python-self-test-readers-decoders/test_body.sh b/qa/TL0_python-self-test-readers-decoders/test_body.sh
index 61c948cf..98b2a124 100644
--- a/qa/TL0_python-self-test-readers-decoders/test_body.sh
+++ b/qa/TL0_python-self-test-readers-decoders/test_body.sh
@@ -1,7 +1,7 @@
 #!/bin/bash -e
 
 test_nose() {
-    for test_script in $(ls test_operator_readers_*.py test_operator_decoders_*.py \
+    for test_script in $(ls test_operator_readers_*.py test_operator_decoders_audio.py \
       test_external_source_dali.py test_external_source_numpy.py \
       test_pool.py test_external_source_parallel_shared_batch.py); do
         nosetests --verbose --attr '!slow' ${test_script}
