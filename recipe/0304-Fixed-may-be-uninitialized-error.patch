From: Nishidha Panpaliya <npanpa23@in.ibm.com>
Date: Mon, 20 Jun 2022 08:01:59 +0000
Subject: [PATCH] Fixed may be uninitialized error

---
 dali/kernels/imgproc/structure/connected_components.h | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/dali/kernels/imgproc/structure/connected_components.h b/dali/kernels/imgproc/structure/connected_components.h
index 739a995d..87ad0c7c 100644
--- a/dali/kernels/imgproc/structure/connected_components.h
+++ b/dali/kernels/imgproc/structure/connected_components.h
@@ -167,13 +167,13 @@ enable_if_t<(ndim > 1)> LabelSlice(OutLabel *label_base,
   for (int64_t i = 0; i < n; i++) {
     auto out_slice = out.slice(i);
     auto in_slice = in.slice(i);
+    prev_out = out_slice;
+    prev_in  = in_slice;
     LabelSlice(label_base, out_slice, in_slice, background, engine);
 
     if (i > 0)
       MergeSlices(label_base, prev_out, out_slice, prev_in, in_slice);
 
-    prev_out = out_slice;
-    prev_in  = in_slice;
   }
 }
 
